<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<display-name>RSImageService</display-name>
	<!-- Cache Servlet -->
	<servlet>
		<servlet-name>GetImage</servlet-name>
		<servlet-class>com.rstech.image.service.GetImage</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<!-- Servlet Mappings -->
	<servlet-mapping>
		<servlet-name>GetImage</servlet-name>
		<url-pattern>/GetImage</url-pattern>
	</servlet-mapping>
</web-app>
